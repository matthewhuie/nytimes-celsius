{
  "name": "NYTimes in Celsius",
  "description": "Convert weather units on NYTimes.com to Celsius",
  "version": "0.1.1",
  "manifest_version": 3,
  "content_scripts": [
   {
     "matches": [ "*://*.nytimes.com/*" ],
     "js": [ "lib/jquery-3.6.0.slim.min.js",
             "lib/jquery-observe.min.js",
             "nytimes-celsius.js" ]
   }
 ],
 "icons": { "192": "icons/nytimes-celsius-192px.png" }
}
